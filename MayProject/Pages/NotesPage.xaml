<UserControl x:Class="MayProject.Pages.NotesPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MayProject.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             Height="644" Width="1114.5" Background="Gray">
    
     <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Notes" FontSize="35" Foreground="Black" Grid.Row="0" Margin="10, 0, 0, 0"/>
        <ScrollViewer  Grid.Row="1">
            <WrapPanel Name="Container" Background="Gray"/>
        </ScrollViewer>
    </Grid>
</UserControl>
